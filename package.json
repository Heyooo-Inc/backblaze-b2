{
  "name": "@heyooo-inc/backblaze-b2",
  "version": "0.0.2",
  "description": "Backblaze B2 SDK",
  "author": "Heyooo-Inc",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Heyooo-Inc/backblaze-b2.git"
  },
  "bugs": {
    "url": "https://github.com/Heyooo-Inc/backblaze-b2/issues"
  },
  "homepage": "https://github.com/Heyooo-Inc/backblaze-b2#readme",
  "main": "index.js",
  "module": "index.mjs",
  "types": "index.d.ts",
  "scripts": {
    "build": "tsup",
    "test": "jest",
    "cov": "jest --coverage",
    "type-check": "tsc --pretty --noEmit",
    "format": "prettier --write ./src",
    "lint": "eslint . --ext .ts --quiet --fix",
    "prebuild": "rimraf lib"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run type-check"
    }
  },
  "lint-staged": {
    "*.ts": [
      "npm run lint",
      "npm run format"
    ]
  },
  "engines": {
    "node": ">=16"
  },
  "dependencies": {
    "@heyooo-inc/utils": "^0.0.4",
    "axios": "^1.4.0",
    "axios-retry": "^3.5.0"
  },
  "peerDependencies": {
    "@heyooo-inc/utils": "^0.0.4",
    "axios": "^1.4.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.22.4",
    "@types/jest": "^29.5.1",
    "@types/node": "^20.1.1",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "babel-jest": "^29.5.0",
    "coveralls": "^3.1.1",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.1.0",
    "tslib": "^2.5.0",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4"
  },
  "jest": {
    "preset": "ts-jest",
    "transform": {
      "^.+\\.(ts|tsx)?$": "ts-jest",
      "^.+\\.(js|jsx)$": "babel-jest"
    },
    "transformIgnorePatterns": [],
    "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "node"
    ]
  }
}
